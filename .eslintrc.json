{
  "root": true,
  "ignorePatterns": ["**/*"],
  "plugins": [],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "error",
          {
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_",
            "caughtErrorsIgnorePattern": "^_"
          }
        ]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "extends": [],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": [],
      "rules": {}
    }
  ]
}
